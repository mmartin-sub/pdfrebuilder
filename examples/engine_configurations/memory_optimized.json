{
    "description": "Memory-optimized configuration for resource-constrained environments",
    "use_case": "Limited memory environments, embedded systems, cloud functions",
    "default_engine": "reportlab",
    "reportlab": {
        "compression": 9,
        "page_mode": "portrait",
        "embed_fonts": true,
        "font_subsetting": true,
        "image_compression": "jpeg",
        "color_space": "rgb",
        "precision": 0.5
    },
    "pymupdf": {
        "overlay_mode": false,
        "annotation_mode": "ignore",
        "compression": "flate",
        "image_quality": 60,
        "text_rendering_mode": "fill",
        "anti_aliasing": false,
        "optimize_for_web": true
    },
    "performance": {
        "enable_caching": false,
        "cache_size": 10,
        "parallel_rendering": false,
        "max_workers": 1
    },
    "debugging": {
        "enable_metrics": false,
        "log_rendering_details": false,
        "save_intermediate_files": false
    }
}
