{
    "description": "Speed-optimized configuration for high-volume document processing",
    "use_case": "Batch processing, high-throughput scenarios",
    "default_engine": "pymupdf",
    "reportlab": {
        "compression": 1,
        "page_mode": "portrait",
        "embed_fonts": false,
        "font_subsetting": false,
        "image_compression": "jpeg",
        "color_space": "rgb",
        "precision": 0.5
    },
    "pymupdf": {
        "overlay_mode": false,
        "annotation_mode": "ignore",
        "compression": "none",
        "image_quality": 75,
        "text_rendering_mode": "fill",
        "anti_aliasing": false,
        "optimize_for_web": false
    },
    "performance": {
        "enable_caching": true,
        "cache_size": 200,
        "parallel_rendering": true,
        "max_workers": 8
    },
    "debugging": {
        "enable_metrics": true,
        "log_rendering_details": false,
        "save_intermediate_files": false
    }
}
